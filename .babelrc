{
  "presets": [
    "stage-1",
    "flow",
    "react"
  ],
  "plugins": [
    "flow-react-proptypes"
  ],
  "env": {
    "test": {
      "presets": [
        ["env", {"targets": {"node": "current"}}],
        "stage-1", "flow", "react"
      ],
      "plugins": [
        "transform-runtime"
      ]
    },
    "coverage": {
      "presets": [
        ["env", {"targets": {"node": "current"}}],
        "stage-1", "flow", "react"
      ],
      "plugins": ["istanbul"]
    },
    "es5": {
      "presets": [
        ["env", {"forceAllTransforms": true}],
        "stage-1", "flow", "react"
      ],
      "plugins": [
        "transform-runtime",
        "transform-react-constant-elements"
      ]
    },
    "production": {
      "plugins": [
        "transform-react-constant-elements"
      ]
    }
  }
}
